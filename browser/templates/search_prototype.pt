  <body id="search_prototype">
    <h1>Search results for OnlineGroups.Net</h1>
    
    <form id="search">
      <div id="keywordsWidgets">
        <label id="keywordsLabel" for="keywords">Search for</label>
        <input type="text" name="keywords" tabindex="1" />
      </div>
      <div id="timePeriodWidgets">
        <label id="timeLabel" for="timePeriod">from</label>
        <select name="timePeriod" tabindex="2">
          <option label="today" value="today" >today</option>
          <option label="todayAndYesterday" value="todayAndYesterday" >today 
            and yesterday</option>
          <option label="thisWeek" value="thisWeek" >the past week</option>
          <option label="thisMonth" value="thisMonth" >the past month</option>
          <option label="thisYear" value="thisYear" >the past year</option>
          <option label="ever" value="ever" >all time</option>
        </select>
      </div>
      <input type="submit" tabindex="3" value="Go"/>
    </form>
    
    <div class="xhtml2-section" id="topics">
      <h2>Topics</h2>
      <ul class="showOnly">
        <li>
          <a href="#">Only search topics</a>
        </li>
        <li>
          <a href="#">Only search posts</a>
        </li>
        <li>
          <a href="#" class="webFeed">Web feed</a>
        </li>
        <li>
          <a title="Put this list on the home-page of OnlineGroups.Net" 
            href="#" class="addToHome">Put on Home</a>
        </li>
      </ul>
      
      <ul id="topicsList" class="searchResults">
          <tal:block tal:repeat="i python:range(0, 10)">
            <li tal:replace="structure provider:groupserver.TopicsSearchItem"/>
          </tal:block>
      </ul>
      <p class="more">
        <a href="#">More topics</a>
      </p>
    </div><!--#topics-->
    
